<node id="Compliance">
  <data key="d0">Compliance</data>
  <data key="d1">category</data>
  <data key="d2">Compliance refers to the adherence of public accountants to the professional standards set by the Oversight Committee.</data>
  <data key="d3">chunk-5bbf3979f42997871ec0460af2162c84</data>
  <data key="d4">unknown_source</data>
